<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
    <!-- bean definitions here -->
    <!-- 开启自动化注入-->
    <context:annotation-config />

    <bean id="userDao" class="com.be.self.dao.UserDao" />
    <bean id="userService" class="com.be.self.service.UserService"></bean>
    <bean id="userDaoIml01" class="com.be.self.dao.UserDaoIml01" />
    <bean id="userDaoIml02" class="com.be.self.dao.UserDaoIml02" />
    <bean id="accountDao" class="com.be.self.dao.AccountDao" />
    <bean id="accountService" class="com.be.self.service.AccountService" />


</beans>
